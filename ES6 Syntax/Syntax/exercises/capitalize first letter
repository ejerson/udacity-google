const days = ['sunday', 'monday', 'tuesday', 'wednesday', 'thursday', 'friday', 'saturday'];

// your code goes here
for (const day of days) {
    let char = day.substring(0, 1).toUpperCase();
    let rest = day.substring(1, day.length);
    let fullDay = `${char}${rest}`


    console.log(fullDay);
}
